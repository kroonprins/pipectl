{
  "name": "@kroonprins/pipectl-azure-devops-imperative-extensions",
  "version": "0.0.1-alpha.5",
  "description": "imperative extensions for the pipectl plugin for Azure DevOps",
  "scripts": {
    "build": "tsc",
    "test": "jest --passWithNoTests",
    "updates": "updates -e @kroonprins/pipectl",
    "updates-apply": "updates -e @kroonprins/pipectl -u"
  },
  "files": [
    "dist/**/!(*.test).js",
    "dist/**/!(*.test).d.ts"
  ],
  "devDependencies": {
    "@kroonprins/pipectl": "0.0.1-alpha.5",
    "@kroonprins/pipectl-azure-devops": "0.0.1-alpha.5",
    "updates": "10.2.13"
  },
  "dependencies": {
    "azure-devops-node-api": "10.1.1",
    "yargs": "15.4.0"
  },
  "peerDependencies": {
    "@kroonprins/pipectl": "*",
    "@kroonprins/pipectl-azure-devops": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
